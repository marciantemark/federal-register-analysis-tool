{
  "name": "federal-register-analysis-tool",
  "version": "1.0.0",
  "description": "Web platform for analyzing Federal Register public comments",
  "type": "module",
  "scripts": {
    "api": "cd api && bun run dev",
    "serve": "bun --serve --port 8000 interfaces/",
    "dev": "concurrently \"bun run api\" \"bun run serve\"",
    "build": "echo 'Building for production...'",
    "setup": "cd api && bun install"
  },
  "workspaces": ["api"],
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "keywords": ["federal-register", "public-comments", "analysis", "visualization"],
  "author": "Your Team", 
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/federal-register-analysis-tool.git"
  }
}
